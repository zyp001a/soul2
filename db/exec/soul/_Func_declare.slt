~
Arr#vts = #0.funcVarTypes
Arr_Str#vars = #0.funcVars
Class#ret = #0.funcReturn
#typemap = getExec()._typemap
#varname = getExec()._varname

#argvx = ""
@each i v vts{
 @if (i != 0) {argvx += ", "}
 @if(!vars){
  argvx += typemap(class(v)) 
 }@else{
  argvx += varname(vars[i]) + " " + typemap(class(v))
 }
}
#returnx = ""
@if(ret){
 returnx += " " + typemap(ret)
}

~->(~=argvx~)~=returnx~